{
  "name": "obj-walker",
  "version": "1.8.0",
  "description": "Walk or map over objects in a depth-first preorder or postorder manner.",
  "author": "David Sargeant",
  "repository": "git://github.com/dubiousdavid/obj-walker.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run lint && npm test && npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --declaration",
    "test": "jest",
    "lint": "eslint src/**"
  },
  "keywords": [
    "tree",
    "map",
    "mapper",
    "object",
    "walk",
    "walker",
    "preorder",
    "postorder",
    "depth-first",
    "search",
    "json",
    "pointer",
    "ref",
    "flatten",
    "find",
    "leaves",
    "traverse",
    "compact",
    "truncate",
    "async"
  ],
  "license": "ISC",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "json-decycle": "^2.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
